<?php
	$categories = array();
	for ($i = 0; $i < 10; ++$i) {
		$categories[$i] = array();
		for ($j = 0; $j < 10; ++$j) {
			$categories[$i][$j] = $j;
		}
	}	
?>

<?php foreach ($categories as $key => $value) {
	echo "<h1>".$key."</h1>";
	foreach ($value as $thing) {
		echo '<a href="/mock/'.$category.'/'.$thing.'">'.'</a><br>';
	}
}
?>
